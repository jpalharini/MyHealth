<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    
  <style name="AutoCompleteTextViewMyHealth" parent="android:Widget.Holo.Light.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/myhealth_list_selector_holo_light</item>
	  <item name="android:background">@drawable/myhealth_edit_text_holo_light</item>
  </style>
  
  <style name="MyActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@color/myhealth_color</item>
        <item name="android:titleTextStyle">@style/MyActionBarText</item>
		<item name="android:icon">@android:color/transparent</item>
  </style>
 
  <style name="MyActionBarText" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@color/button_text_color</item>
        <item name="android:textSize">25sp</item>
  </style>  

  <style name="ButtonMyHealth" parent="android:Widget.Holo.Light.Button">
	  <item name="android:background">@drawable/myhealth_btn_default_holo_light</item>
	  <item name="android:textColor">@color/button_text_color</item>
	  <item name="android:padding">12dp</item>
  </style>

  <style name="ImageButtonMyHealth" parent="android:Widget.Holo.Light.ImageButton">
	  <item name="android:background">@drawable/myhealth_btn_default_holo_light</item>
  </style>

  <style name="SpinnerMyHealth" parent="android:Widget.Holo.Light.Spinner">
      <item name="android:background">@drawable/myhealth_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/myhealth_list_selector_holo_light</item>
  </style>

  <style name="EditTextMyHealth" parent="android:Widget.Holo.Light.EditText">
      <item name="android:textSize">30sp</item>
      <item name="android:layout_marginTop">20dp</item>
  </style>
  
  <style name="TabMyHealth">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/myhealth_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextMyHealth">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarMyHealth" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/myhealth_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/myhealth_progress_indeterminate_horizontal_holo_light</item>
  </style>

  <style name="ToggleMyHealth" parent="android:Widget.Holo.Light.Button.Toggle">
      <item name="android:background">@drawable/myhealth_btn_toggle_holo_light</item>
  </style>

</resources>